$text = widget["tutorialAnswer"].element["0001"].text;
if $text == "2" then
  display_message("Foarte bine!");
  global.property["questionCompleted"] = true;
  if global.property["tutorialBattlePhase"] == 0 then
    start_dialogue("powerTutorial");
  else
    display_message("Felicitari, ai terminat tutorialul! Acum poti intra in joc!");
    global.property["receiver"] = "answer";
    execute_script("game", false);
  end;
else
  display_message("Raspunsul este gresit, mai incearca!");
end;

show_widget("answer");
show_widget("timer");
$dat = data[global.property["fighter"] + ".json"];
$rounds = random(data["min"], data["max"]);
for $i in range(0, $rounds) do
  $percent = random(1, 100);
  $nr = 0;
  while true do
    if $dat["attacks"][$nr] >= $percent then
      break;
    else
      $nr += 1;
    end;
  end;
end;

$battleNr = 4;
if global.property["fought"] >= $battleNr then
	global.property["battleID"] = "battle_05.json";
	execute_script("battle");
	global.property["preScript"] = "";
	global.property["posScript"] = "";
	global.property["map"] = "Cave 1";
